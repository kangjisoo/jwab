<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="org.techtown.loginactivity"
    >

    <uses-permission android:name="android.permission.INTERNET" />
    <!-- camera, gallery -->
    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE"/>


    <application
        android:allowBackup="true"
        android:hardwareAccelerated="true"
        android:icon="@mipmap/ic_launcher"
        android:label="Jwab"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true"
        android:requestLegacyExternalStorage="true"
        android:screenOrientation="portrait">

        <uses-library
            android:name="org.apache.http.legacy"
            android:required="false"
            android:screenOrientation="portrait" />

        <activity android:name=".LoadingActivity"
            android:theme="@style/SplashTheme"
            android:screenOrientation="portrait"/>


        <activity
            android:name="org.techtown.board.BoardAddTest"
            android:screenOrientation="portrait"
            >
        </activity>

        <activity
            android:name="org.techtown.board.BoardImgClick"
            android:screenOrientation="portrait"
            >
        </activity>
        <activity
            android:name="org.techtown.projectmain.ProjectHomeRecyclerView"
            tools:ignore="Instantiatable"
            android:screenOrientation="portrait">
        </activity>

        <activity
            android:name="org.techtown.projectmain.MemberAdd"
            android:label="멤버 초대하기"
            android:theme="@style/Theme.AppCompat.Dialog"
            android:screenOrientation="portrait">
        </activity>


        <activity android:name="org.techtown.projectinner.InnerMainRecycler"
            android:label="projectInner"
            tools:ignore="Instantiatable"
            android:screenOrientation="portrait">

        </activity>

        <activity
            android:name="org.techtown.board.BoardMainRecycler"
            android:screenOrientation="portrait">
        </activity>

        <activity
            android:name="org.techtown.board.BoardView"
            android:screenOrientation="portrait">
        </activity>

        <activity
            android:name="org.techtown.projectmain.ProjectHome"
            android:screenOrientation="portrait"
           />
        <activity
            android:name=".SignActivty"
            android:label="회원가입"
            android:theme="@style/Theme.AppCompat.Dialog"
            android:screenOrientation="portrait"/>




        <activity android:name="org.techtown.randomgame.RandomGameFirst"
            android:screenOrientation="portrait">

        </activity>
        <activity android:name="org.techtown.randomgame.RandomGameSecond"
            android:screenOrientation="portrait"/>
        <activity
            android:name=".MainActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

       <activity
           android:name="org.techtown.calendar.innercalendar"
           android:screenOrientation="portrait">

       </activity>


        <activity android:name="org.techtown.vote.VoteMain"
            android:screenOrientation="portrait">

        </activity>

        <activity android:name="org.techtown.vote.VoteFinish"
            android:screenOrientation="portrait">

        </activity>

        <activity android:name="org.techtown.vote.VoteDP"
            android:screenOrientation="portrait">

        </activity>





    </application>

</manifest>